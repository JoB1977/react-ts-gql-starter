{
  "name": "Github GraphQL Schema",
  "schemaPath": "schema.graphql",
  "extensions": {
    "endpoints": {
      "Default GraphQL Endpoint": {
        "url": "https://api.github.com/graphql",
        "headers": {
          "user-agent": "JS GraphQL",
          "Authorization": "Bearer <GITHUB_PERSONAL_ACCESS_TOKEN>"
        },
        "introspect": true
      }
    }
  }
}
